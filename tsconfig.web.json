{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      /*"@registry/*": [
        "src/renderer/src/lib/*"
      ],
      "@components/*": [
        "src/renderer/src/lib/new-york-v4/*"
      ],
      "@lib/*": [
        "src/renderer/src/lib/new-york-v4/lib/*"
      ]*/
    }
  }
}
